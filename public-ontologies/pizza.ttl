      owl:intersectionOf (
          :Pizza
          [
            a owl:Restriction ;
            owl:onProperty :hasTopping ;
            owl:someValuesFrom :SpicyTopping ;
          ]
        ) ;
    ] ;
  skos:definition "Any pizza that has a spicy topping is a SpicyPizza"@en ;
  skos:prefLabel "Spicy Pizza"@en ;
